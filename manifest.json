{
  "name": "Help-Chat",
  "description": "Help-Chat",
  "version": "0.1",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": ["fullscreen", "alwaysOnTopWindows", "http://news.google.com/", "webview", "alwaysOnTopWindows", "storage", "fullscreen" ],
  "icons": { "16": "my-app/public/logo.png",
    "48": "my-app/public/logo.png",
    "128": "my-app/public/logo.png" }
}